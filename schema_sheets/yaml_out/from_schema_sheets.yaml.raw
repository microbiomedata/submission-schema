classes:
  AirInterface:
    description: air dh_interface
    from_schema: https://example.com/nmdc_submission_schema
    is_a: DhInterface
    mixins:
    - DhMutliviewCommonColumnsMixin
    - SampIdNewTermsMixin
    title: Air
  BiofilmInterface:
    description: biofilm dh_interface
    from_schema: https://example.com/nmdc_submission_schema
    is_a: DhInterface
    mixins:
    - DhMutliviewCommonColumnsMixin
    - SampIdNewTermsMixin
    title: Biofilm
  BuiltEnvInterface:
    description: built_env dh_interface
    from_schema: https://example.com/nmdc_submission_schema
    is_a: DhInterface
    mixins:
    - DhMutliviewCommonColumnsMixin
    - SampIdNewTermsMixin
    title: Built Env
  DhInterface:
    description: One DataHarmonizer interface, for the specified combination of a
      checklist, enviornmental_package, and various standards, user facilities or
      analysis types
    from_schema: https://example.com/nmdc_submission_schema
    title: Dh Root Interface
  DhMutliviewCommonColumnsMixin:
    description: Mixin with DhMutliviewCommon Columns
    from_schema: https://example.com/nmdc_submission_schema
    mixin: true
    slots:
    - analysis_type
    title: Dh Mutliview Common Columns
  EmslInterface:
    description: emsl dh_interface
    from_schema: https://example.com/nmdc_submission_schema
    is_a: DhInterface
    mixins:
    - DhMutliviewCommonColumnsMixin
    slots:
    - emsl_store_temp
    - project_id
    - replicate_number
    - sample_shipped
    - sample_type
    - technical_reps
    title: Emsl
  HcrCoresInterface:
    description: hcr_cores dh_interface
    from_schema: https://example.com/nmdc_submission_schema
    is_a: DhInterface
    mixins:
    - DhMutliviewCommonColumnsMixin
    title: Hcr Cores
  HcrFluidsSwabsInterface:
    description: hcr_fluids_swabs dh_interface
    from_schema: https://example.com/nmdc_submission_schema
    is_a: DhInterface
    mixins:
    - DhMutliviewCommonColumnsMixin
    title: Hcr Fluids Swabs
  HostAssociatedInterface:
    description: host_associated dh_interface
    from_schema: https://example.com/nmdc_submission_schema
    is_a: DhInterface
    mixins:
    - DhMutliviewCommonColumnsMixin
    - SampIdNewTermsMixin
    title: Host Associated
  JgiMgInterface:
    description: jgi_mg dh_interface
    from_schema: https://example.com/nmdc_submission_schema
    is_a: DhInterface
    mixins:
    - DhMutliviewCommonColumnsMixin
    slots:
    - dna_absorb1
    - dna_absorb2
    - dna_collect_site
    - dna_concentration
    - dna_cont_type
    - dna_cont_well
    - dna_container_id
    - dna_dnase
    - dna_isolate_meth
    - dna_organisms
    - dna_project_contact
    - dna_samp_id
    - dna_sample_format
    - dna_sample_name
    - dna_seq_project
    - dna_seq_project_name
    - dna_seq_project_pi
    - dna_volume
    - proposal_dna
    title: JGI MG
  JgiMtInterface:
    description: jgi_mt dh_interface
    from_schema: https://example.com/nmdc_submission_schema
    is_a: DhInterface
    mixins:
    - DhMutliviewCommonColumnsMixin
    slots:
    - dnase_rna
    - proposal_rna
    - rna_absorb1
    - rna_absorb2
    - rna_collect_site
    - rna_concentration
    - rna_cont_type
    - rna_cont_well
    - rna_container_id
    - rna_isolate_meth
    - rna_organisms
    - rna_project_contact
    - rna_samp_id
    - rna_sample_format
    - rna_sample_name
    - rna_seq_project
    - rna_seq_project_name
    - rna_seq_project_pi
    - rna_volume
    title: JGI MT
  MiscEnvsInterface:
    description: misc_envs dh_interface
    from_schema: https://example.com/nmdc_submission_schema
    is_a: DhInterface
    mixins:
    - DhMutliviewCommonColumnsMixin
    title: Misc Envs
  PlantAssociatedInterface:
    description: plant_associated dh_interface
    from_schema: https://example.com/nmdc_submission_schema
    is_a: DhInterface
    mixins:
    - DhMutliviewCommonColumnsMixin
    - SampIdNewTermsMixin
    title: Plant Associated
  SampIdNewTermsMixin:
    description: Mixin with SampIdNew Terms
    from_schema: https://example.com/nmdc_submission_schema
    mixin: true
    slots:
    - env_package
    - sample_link
    title: SampId New Terms
  SampleData:
    description: represents data produced by the DataHarmonizer tabs of the submission
      portal
    from_schema: https://example.com/nmdc_submission_schema
    slots:
    - air_data
    - biofilm_data
    - built_env_data
    - host_associated_data
    - plant_associated_data
    - sediment_data
    - soil_data
    - wastewater_sludge_data
    - water_data
    - emsl_data
    - jgi_mg_data
    - jgi_mt_data
    title: SampleData
    tree_root: true
  SedimentInterface:
    description: sediment dh_interface
    from_schema: https://example.com/nmdc_submission_schema
    is_a: DhInterface
    mixins:
    - DhMutliviewCommonColumnsMixin
    - SampIdNewTermsMixin
    title: Sediment
  SoilInterface:
    description: soil dh_interface
    from_schema: https://example.com/nmdc_submission_schema
    is_a: DhInterface
    mixins:
    - DhMutliviewCommonColumnsMixin
    - SampIdNewTermsMixin
    - SoilMixsInspiredMixin
    title: Soil
  SoilMixsInspiredMixin:
    description: Mixin with SoilMixsInspired Terms
    from_schema: https://example.com/nmdc_submission_schema
    mixin: true
    slots:
    - collection_date_inc
    - collection_time
    - collection_time_inc
    - experimental_factor_other
    - filter_method
    - isotope_exposure
    - micro_biomass_c_meth
    - micro_biomass_n_meth
    - microbial_biomass_c
    - microbial_biomass_n
    - non_microb_biomass
    - non_microb_biomass_method
    - org_nitro_method
    - other_treatment
    - start_date_inc
    - start_time_inc
    title: Soil MIxS Inspired Mixin
  WastewaterSludgeInterface:
    description: wastewater_sludge dh_interface
    from_schema: https://example.com/nmdc_submission_schema
    is_a: DhInterface
    mixins:
    - DhMutliviewCommonColumnsMixin
    - SampIdNewTermsMixin
    title: Wastewater Sludge
  WaterInterface:
    description: water dh_interface
    from_schema: https://example.com/nmdc_submission_schema
    is_a: DhInterface
    mixins:
    - DhMutliviewCommonColumnsMixin
    - SampIdNewTermsMixin
    title: Water
